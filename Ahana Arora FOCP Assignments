#include <stdio.h>
int main() {
    int n, 1. j, value;
    printf("Enter the number of rows for Pascal's Triangle: ");
    scanf("%d",&n);
    for(i=0; i<n; i++) {
        value = 1;
        for(j=0;j<n-1; j++) {
            printf("");
        }
        for(j=0; j<=i; j++) {
           printf("%d", value);
           if (j<i) {
               value= value*(i-j)/(j+1);
           }
         }
         printf("\n");
    }
    return 0;
}
