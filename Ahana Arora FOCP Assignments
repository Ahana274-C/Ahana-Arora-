#include <stdio.h>
int main() {
   float x, y;
   printf("Enter the coordinates (x y): ");
   scanf("%f %f", &x, &y):
   if ( x > 0 && y > 0 ) {
       printf("The point (%f,%f) lies in Quadrant 1.\n", x,y);
   } else if ( x < 0 && y > 0 ) {
       printf("The point (%f,%f) lies in Quadrant II.\n", x, y);
   } else if ( x < 0 8 y < 0 ) {
       printf("The point (%f,%f) lies in Quadrant III.\n", x, y);
   } else if (x > 0 && y < 0 ) {
       printf("The point (%f,%f) lies in Quadrant IV.\n", x, y);
   } else if (x==0 y==0 ) {
       printf("The point (%f,%f) is at the origin.\n", x, y);
   } else if (x==0) {
       printf("The point (%f,%f) lies on the Y-axis.\n", x, y);
   } else {
       printf("The point (%f,%f) lies on the X-axis.in", x, y);
   }
   return 0;
}
